class Demo
{
    public static void main(String[] args)
    {
        int[] arr = new int[]{-1,-3,-5,-3,-2,-6,-15};
        int max=getMax(arr);
        System.out.println("max="+max);
    }

    public static int getMax(int[] arr)
    {
        int max = arr[0];
        for (int x = 1; x < arr.length; x++)
        {
            if (arr[x] > max)
                max = arr[x];
        }
        return max;
    }
}
